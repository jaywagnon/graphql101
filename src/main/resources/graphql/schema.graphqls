schema {
    query: Query
}

type Author {
    name: String!

    books: [Book!]!
}

type Book {
    name: String!

    author: Author!
}

type Query {
    "Simply returns 'true'.  A no-op query to satisfy the minimum requirements for a GraphQL schema."
    healthCheck: Boolean!

    getAuthors: [Author!]!

    getBooks: [Book!]!
}
